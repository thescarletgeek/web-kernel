{
    "name": "@thescarletgeek/web-kernel",
    "version": "1.0.0",
    "description": "A kernel for web to handle scripting",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "directories": {
        "example": "example"
    },
    "repository": {
        "type": "git",
